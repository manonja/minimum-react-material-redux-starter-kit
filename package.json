{
  "name": "minimum-bootstrap-starter-kit",
  "version": "1.0.0",
  "description": "bootstrap-starter-kit",
  "main": "index.js",
  "scripts": {
    "start": "serve --listen ${PORT:-8080} src",
    "test": "run-p --print-label --race start cy:run",
    "stop-only": "stop-only --folder examples --skip node_modules",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cypress:verify": "cypress verify",
    "cy:run:record": "cypress run --record ",
    "e2e": "cypress run",
    "start:ci": "serve --listen ${PORT:-8080} src",
    "test:ci": "run-p --race --print-label start:ci e2e", 
    "test:ci:chrome": "run-p --race start:ci cypress:run:chrome",
    "cypress:run:chrome": "cypress run --browser chrome"
  },
